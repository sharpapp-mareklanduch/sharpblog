@model IEnumerable<SharpBlog.Core.Models.PostDto>
<div class="container">
    <div class="row">
        <div class="offset-2 col-8">
            <h4 class="text-center">Category: @ViewData["category"]</h4>
        </div>
    </div>
    @foreach (var post in Model)
    {
        await Html.RenderPartialAsync("~/Views/Blog/_Post.cshtml", post);
    }
</div>